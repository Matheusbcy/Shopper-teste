{
  "compilerOptions": {
    "types": ["node", "express", "multer"],
    "target": "ES6",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./build",
    "rootDir": "./src",
    "removeComments": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "strict": true
  }
}
